{
  "root": true,
  "extends": [
    "@react-native",
    "airbnb",
    "airbnb-typescript",
    "prettier",
  ],
  "plugins": [
    "@typescript-eslint",
    "react",
    "react-native"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".jsx",
          ".tsx"
        ]
      }
    ]
  }
}